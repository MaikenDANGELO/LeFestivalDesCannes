{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions, mapState } from 'vuex';\nexport default {\n  name: \"PageAdministrateur\",\n  data: () => ({\n    showServices: []\n  }),\n  computed: {\n    ...mapState('prestataire', ['prestataires'])\n  },\n  methods: {\n    ...mapActions('prestataire', ['getAllPrestataires']),\n    handlePrestaGoToPage(id) {\n      this.$router.push(\"/prestataire/\" + id);\n    },\n    handleShowServices(id) {\n      this.showServices[id] = !this.showServices[id];\n    }\n  },\n  created() {\n    this.getAllPrestataires();\n    this.showServices = Array.from({\n      length: this.prestataires.length\n    }, () => false);\n  }\n};","map":{"version":3,"names":["mapActions","mapState","name","data","showServices","computed","methods","handlePrestaGoToPage","id","$router","push","handleShowServices","created","getAllPrestataires","Array","from","length","prestataires"],"sources":["src/views/PageAdministrateur.vue"],"sourcesContent":["<template>\n    <div>\n        <h1 style=\"text-align: center;\">Page d'administration</h1>\n        <div class=\"list-prestataires\">\n            <div class=\"switch-admin\">\n                <button><h2>Sponsors</h2></button>\n                <button><h2>Prestataires</h2></button>\n            </div>\n            <div class=\"prestataire\" v-for=\"prestataire in this.getPrestataires()\" :key=\"prestataire.id\">\n                <div class=\"presta-top\"><h3>{{ prestataire.nom }}</h3></div>\n                <div class=\"presta-sbody\">\n                    <div class=\"presta-body\">\n                        <div class=\"presta-icon\"><img class=\"prestataire-img\" alt=\"prestimg\" :src=\"require(`../assets/${prestataire.image}`)\" /></div>\n                        <div class=\"presta-text\">{{ prestataire.description_accueil }}</div>\n                        <div class=\"presta-actions\">\n                            <button @click=\"handlePrestaGoToPage(prestataire.id)\">Accéder à la page</button>\n                            <button>action..</button>\n                            <button>action..</button>\n                            <button>action..</button>\n                        </div>\n                    </div>\n                    <div class=\"presta-bottom\">\n                        <button class=\"show-services\" @click=\"handleShowServices()\">Afficher services</button>\n                        <div v-if=\"showServices[prestataire.id]\">\n                            <button v-for=\"service in prestataire.services\" :key=\"service.id\">{{ service.nom }}</button>\n                        </div>\n                    </div>\n                </div>\n                \n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { mapActions, mapState } from 'vuex';\n\n    export default {\n        name: \"PageAdministrateur\",\n        data : () => ({\n            showServices: [],\n        }),\n        computed: {\n            ...mapState('prestataire',['prestataires']),\n        },\n        methods: {\n            ...mapActions('prestataire', ['getAllPrestataires']),\n            handlePrestaGoToPage(id){\n                this.$router.push(\"/prestataire/\"+id);\n            },\n            handleShowServices(id){\n                this.showServices[id] = !this.showServices[id];\n            }\n        },\n        created() {\n            this.getAllPrestataires();\n            this.showServices = Array.from({ length: this.prestataires.length }, () => false)\n        },\n    }\n</script>\n\n<style scoped>\n    .list-prestataires{\n        display: flex;\n        flex-direction: column;\n        margin: 5%;\n    }\n    .prestataire{\n        display: flex;\n        flex-direction: column;\n        margin-top: 1%;\n        margin-bottom: 1%;\n    }\n    .presta-sbody{\n        border: 1px solid black;\n        border-radius: 5px;\n    }\n    .presta-body{\n        display: flex;\n        flex-direction: row;\n      